{
    "name": "people",
    "type": "Microsoft.Common.EditableGrid",
    "ariaLabel": "Enter information per person",
    "label": "DNS",
    "constraints": {
      "width": "50",
      "rows": {
        "count": {
          "min": 1,
          "max": 10
        }
      },
      "columns": [
        {
          "id": "colDNS",
          "header": "DNS",
          "width": "1fr",
          "element": {
            "type": "Microsoft.Common.TextBox",
            "placeholder": "10.10.10.10",
            "constraints": {
              "required": true,
              "validations": [
                {
                  "regex": "^(?:[0-9]{1,3}\\.){3}[0-9]{1,3}$",
                  "message": "Must be a valid IP address"
                }
              ]
            }
          }
        }
      ]
    }
  },